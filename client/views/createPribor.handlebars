            <form method="post" action="createPribor/add">
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Наименование счётчика</label>
                    <input type="nameMeter" name="nameMeter" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                </div>
                <div class="row mt-3">
                    <div class="col-6">
                        <select class="form-select" name="brand" aria-label="Default select example">
                            <option selected>Выбрать бренд</option>
                            {{#each brands}}
                                <option value="{{id}}">{{name}}</option>
                            {{/each}}

                        </select>
                        <select class="form-select mt-2" name="typeMeter" aria-label="Default select example">
                            <option selected>Выбрать тип счётчика</option>
                            {{#each typeMeter}}
                                <option value="{{id}}">{{name}}</option>
                            {{/each}}

                        </select>
                    </div>
                </div>

                <button type="submit" class="btn btn-primary mt-3">Добавить</button>
            </form>
            {{#each meters}}
                <div class="row m-2 p-3" style="border: 1px solid grey; border-radius: 15px">
                    <form method="post" action="createMeters/edit/{{id}}">
                        <div class="row">
                            <div class="col-6">
                                <input type="nameMeter" name="nameMeter" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" value="{{name}}">
                            </div>

                        </div>
                        <div class="col-6">
                            <select class="form-select" name="typeMeter" aria-label="Default select example">
                                <option selected>Выбрать тип счётчика</option>
                                <option selected value="{{typeMeterId}}">{{typeMeterName}}</option>
                            </select>
                        </div>

                        <div class="col-6">
                            <select class="form-select" name="brands" aria-label="Default select example">
                                <option selected>Выбрать тип счётчика</option>
                                <option selected value="{{brandId}}">{{brandName}}</option>
                            </select>
                        </div>
                        <div class="row mt-3">
                            <div class="col-3">
                                <button type="submit" class="btn btn-warning">Изменить</button>
                            </div>
                        </div>
                    </form>
                    <div class="col-3">
                        <form method="post" action="createMeters/del/{{id}}">
                            <button type="submit" class="btn btn-danger">Удалить</button>
                        </form>
                    </div>
                </div>
            {{/each}}

