            {{#if number}}

                <div class="row mt-3 p-2" style="border: 1px solid gray; border-radius: 15px">
                    <h5>Секция №{{number}}</h5>
                    <div class="col-6">
                        <form method="post" action="/formSectionObject/{{objectId}}/addParams/{{id}}">
                            <div class="input-group">
                                <span class="input-group-text">Этажи - от и до</span>
                                <input type="number" name="from" value="{{from}}" aria-label="От" class="form-control">
                                <input type="number" name="to" value="{{to}}" aria-label="До" class="form-control">
                            </div>
                            <div class="row">
                                <div class="col-3">
                                    <div class="col-auto">
                                        <label for="inputPassword6" class="col-form-label">Стояков</label>
                                    </div>
                                    <div class="col-auto">
                                        <input type="number" id="inputPassword6" value="{{pipes}}" name="pipes" class="form-control" aria-describedby="passwordHelpInline">
                                    </div>
                                </div>
                                <div class="col-3">
                                    <div class="col-auto">
                                        <label for="inputPassword6" class="col-form-label">Квартир</label>
                                    </div>
                                    <div class="col-auto">
                                        <input type="number" name="flat" value="{{flat}}" id="inputPassword6" class="form-control" aria-describedby="passwordHelpInline">
                                    </div>
                                </div>
                                <div class="col-3">
                                    <div class="col-auto">
                                        <label for="inputPassword6" class="col-form-label">Офисов</label>
                                    </div>
                                    <div class="col-auto">
                                        <input type="number" name="office" value="{{office}}" id="inputPassword6" class="form-control" aria-describedby="passwordHelpInline">
                                    </div>
                                </div>
                                <div class="col-5">
                                    <div class="col-auto">
                                        <label for="inputPassword6" class="col-form-label">Квартир на этаже</label>
                                    </div>
                                    <div class="col-auto">
                                        <input type="number" name="flatInFloor" value="{{flatInFloor}}" id="inputPassword6" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-4">
                                    <button type="submit" class="btn btn-primary mt-2">Отправить</button>
                                </div>

                            </div>
                        </form>
                        <div class="col-4">
                            <a href="/editSection/{{objectId}}/{{id}}">
                                <button class="btn btn-primary mt-2">К секции</button>
                            </a>
                        </div>
                    </div>
                </div>

