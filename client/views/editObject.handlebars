        <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4"><div class="chartjs-size-monitor"><div class="chartjs-size-monitor-expand"><div class=""></div></div><div class="chartjs-size-monitor-shrink"><div class=""></div></div></div>
            <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                <h1 class="h2">Редактируем объект {{name}}</h1>
            </div>



                <div class="row mt-3 m-1 p-3" style="border: 1px solid gray; border-radius: 15px">
                    <form method="post" action="/editObject/edit/{{id}}" class="row g-3" >
                        <div class="col-md-6">
                            <label for="inputEmail4" class="form-label">Наименование</label>
                            <input type="text" name="name" class="form-control" id="inputEmail4" value="{{name}}">
                        </div>
                        <div class="col-md-6">
                            <label for="inputPassword4" class="form-label">Адрес</label>
                            <input type="text" name="address" class="form-control" id="inputPassword4" value="{{address}}">
                        </div>


                        <div class="col-3">
                            <label for="inputAddress" class="form-label">Кол-во секций</label>
                            <input type="number" name="section" class="form-control" id="inputAddress" value="{{section}}">
                        </div>
                        <div class="col-3">
                            <label for="inputAddress2" class="form-label">Кол-во квартир</label>
                            <input type="text" name="flat" class="form-control" id="inputAddress2" value="{{flat}}">
                        </div>
                        <div class="col-md-6">
                            <label for="inputCity" class="form-label">Кол-во офисов</label>
                            <input type="text" name="office"  class="form-control" id="inputCity" value="{{office}}">
                        </div>
                        <div class="row mt-2">
                            {{#if metersInFlat}}
                                <div class="col-3 m-3" style="border: 1px solid gray; border-radius: 15px; text-align: center">
                                    <p>Счётчики воды установлены в квартире</p>
                                </div>
                            {{/if}}
                            {{#if metersInGr}}
                                <div class="col-3 m-3" style="border: 1px solid gray; border-radius: 15px; text-align: center">
                                    <p>Счётчики воды установлен в гребёнке</p>
                                </div>
                            {{/if}}
                            {{#if systemBolid}}
                                <div class="col-3 m-3" style="border: 1px solid gray; border-radius: 15px; text-align: center">
                                    <p>Используется система ЗАО НВП "БОЛИД"</p>
                                </div>
                            {{/if}}
                            {{#if systemTeplovodohran}}
                                <div class="col-3 m-3" style="border: 1px solid gray; border-radius: 15px; text-align: center">
                                    <p>Используется система ООО НПП «ТЕПЛОВОДОХРАН»"</p>
                                </div>
                            {{/if}}
                        </div>
                        <div class="row mt-2">
                            <div class="col-3">
                                <input class="form-check-input" name="metersInFlat" type="checkbox" value="true" id="flexCheckDefault">
                                <label class="form-check-label" for="flexCheckDefault">
                                    Счётчики в квартирах
                                </label>
                            </div>
                            <div class="col-3">
                                <input class="form-check-input "name="metersInGr" type="checkbox" value="true" id="flexCheckChecked">
                                <label class="form-check-label" for="flexCheckChecked">
                                    Счётчики в гребёнке
                                </label>
                            </div>
                        </div>

                        <div class="row mt-2">
                            <h6>Используется системa :</h6>
                            <div class="col-3">
                                <input class="form-check-input" name="systemBolid" type="checkbox" value="true" id="flexCheckDefault">
                                <label class="form-check-label" for="flexCheckDefault">
                                    ЗАО НВП "БОЛИД"
                                </label>
                            </div>
                            <div class="col-3">
                                <input class="form-check-input" name="systemTeplovodohran" type="checkbox" value="true" id="flexCheckDefault">
                                <label class="form-check-label" for="flexCheckDefault">
                                    ООО НПП «ТЕПЛОВОДОХРАН»
                                </label>
                            </div>
                        </div>
                        <div class="col">
                            <button type="submit" class="btn btn-primary">Изменить</button>
                        </div>
                        <div class="col">
                            <button type="submit" class="btn btn-primary">Сформировать секции</button>
                        </div>



                    </form>
